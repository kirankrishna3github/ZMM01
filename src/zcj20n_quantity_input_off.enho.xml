   <SHORTTEXT>Make Quantity Input off if PR is released</SHORTTEXT>
   <ORIGINAL_OBJECT>
    <PGMID>R3TR</PGMID>
    <ORG_OBJ_TYPE>FUGR</ORG_OBJ_TYPE>
    <ORG_OBJ_NAME>COMF</ORG_OBJ_NAME>
